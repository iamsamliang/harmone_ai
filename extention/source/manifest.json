{
  "name": "Harmone AI",
  "description": "AI companion that engages with you while you watch YouTube",
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "cs.js"
      ],
      "match_about_blank": true,
      "matches": [
        "http://*/*",
        "https://*/*",
        "file://*/*",
        "\u003Call_urls>"
      ],
      "run_at": "document_start"
    }
  ],
  "offline_enabled": true,
  "content_security_policy": "script-src 'self' 'unsafe-eval' 'unsafe-hashes' 'unsafe-inline'; object-src 'self' 'unsafe-eval' 'unsafe-hashes' 'unsafe-inline'",
  "icons": {
    "128": "logo.png",
    "16": "logo.png"
  },
  "manifest_version": 2,
  "permissions": [
    "http://*/*",
    "https://*/*",
    "debugger",
    "background",
    "nativeMessaging",
    "tabs",
    "storage",
    "unlimitedStorage",
    "webNavigation",
    "\u003Call_urls>",
    "cookies",
    "proxy",
    "webRequest",
    "webRequestBlocking",
    "browsingData",
    "webRequestAuthProvider",
    "blocking",
    "asyncBlocking",
    "downloads",
    "downloads.open",
    "system.storage",
    "fileSystem",
    "downloads.shelf",
    "downloads.ui",
    "privacy",
    "settings",
    "popups",
    "contentSettings",
    "alarms",
    "contentSettings",
    "downloads",
    "scripting",
    "storage",
    "tabCapture",
    "scripting"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.0"
}

